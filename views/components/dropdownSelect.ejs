<div id="dropdown-custom-angular" ng-mousedown="toggleDropDown($event)" 
		ng-class="::({'disabled': disabled})" class="wrap-dd-select" tabindex="0">
		<span ng-mousedown="toggleDropDown($event)" class="selected" ng-bind="dropdownModel[labelField]"></span>
		<ul class="dropdown">
		<li ng-repeat="item in ::dropdownSelect" class="dropdown-item"
 				dropdown-select-item="::item"
 				dropdown-item-label="labelField">
		</li>
	</ul>
</div>